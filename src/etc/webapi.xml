<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <!-- Walley - Guest - get iframe src -->
    <route url="/V1/guest-carts/walley-payment/:cartId" method="POST">
        <service class="Webbhuset\CollectorCheckout\Api\GetIframeSrcInterface" method="execute"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
